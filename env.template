# Environment Configuration Template
# Copy this to .env.local and fill in your actual values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenAI API Key for AI Coach (Speech-to-Text & GPT-4)
OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API Key for premium text-to-speech (Coach voices)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# RevenueCat API Keys for Premium Features
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your_ios_api_key_here
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your_android_api_key_here
EXPO_PUBLIC_PREMIUM_ENTITLEMENT=premium

# Development flags - Set BYPASS_PREMIUM=false to test premium features
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_BYPASS_PREMIUM=false
